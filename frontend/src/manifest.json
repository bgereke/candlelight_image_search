{
  "name": "candlelight",
  "description": "A Vue.js web extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "identity"
  ],
  "icons": {
    "48": "icons/candle_48.png",
    "128": "icons/candle_128.png"
  },
  "browser_action": {
    "default_title": "candlelight"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "oauth2": {
    "client_id": "316106815506-dbk6jlt80jlh0iotd2enoa127it3fj5a.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/cloud-vision"
    ]
  },
  "key": ""
}
